{
  "name": "fastsite",
  "version": "1.3.0",
  "description": "Tool for deploying static websites to AWS, automatically creating an S3 Bucket and Cloudfront deployment",
  "main": "index.js",
  "author": "vltmn <melkerveltman@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.447.0",
    "cloudformation-js-yaml-schema": "^0.4.2",
    "global": "^4.4.0",
    "js-yaml": "^3.13.1",
    "mime-types": "^2.1.24",
    "mustache": "^3.0.1",
    "slash": "^3.0.0",
    "yargs": "^13.2.2",
    "yesno": "^0.3.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.3",
    "@types/mime-types": "^2.1.0",
    "@types/mustache": "^0.8.32",
    "@types/node": "^11.13.9",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "copyfiles": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "lint": "eslint src/*",
    "handler": "ts-node src/index.ts",
    "build": "tsc && copyfiles -u 1 src/cloudformation.yml.mustache dist/"
  },
  "bin": {
    "fastsite": "dist/index.js"
  },
  "keywords": [
    "website",
    "static",
    "deploy",
    "AWS",
    "deployment",
    "cloudfront",
    "cloudformation",
    "s3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltmn/fastsite.git"
  }
}
